<?xml version='1.0' encoding='utf-8'?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.phonegap.TravelKeeper" version="1.00">
	<name>TravelKeeper</name>
	<description>Keep record of notes</description>
	<author email="B00307448@studentmail.uws.ac.uk" href="#">Sergej Vasiljev</author>

	<access origin="*" />
	<preference phonegap-version="3.6.3" />
	<preference fullscreen="true" />
	<preference splash-screen-duration="10000" />
	<preference android-minSdkVersion="7" />
	<preference android-maxSdkVersion="21" />
	<preference android-installLocation="auto" />
	<preference target-device="universal" />
	<preference webviewbounce="true" />
	<preference show-splash-screen-spinner="true" />
	<preference auto-hide-splash-screen="true" />
	<preference useBrowserHistory="true" />
	<preference exit-on-suspend="false" />
	<gap:plugin name="org.apache.cordova.camera" />
	<gap:plugin name="org.apache.cordova.media-capture" />
	<gap:plugin name="org.apache.cordova.device" />
	<gap:plugin name="org.apache.cordova.media" />
	<gap:plugin name="org.apache.cordova.splashscreen" />
	<gap:plugin name="org.apache.cordova.dialogs" />
</widget>
